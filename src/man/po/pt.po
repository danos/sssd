#: sssd-krb5.5.xml:442
#, no-wrap
msgid ""
"    [domain/FOO]\n"
"    auth_provider = krb5\n"
"    krb5_server = 192.168.1.1\n"
"    krb5_realm = EXAMPLE.COM\n"
msgstr ""

